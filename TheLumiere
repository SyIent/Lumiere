--PlayerTab
local BlueTab = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://7992557371",
	PremiumOnly = false
})
--CharacterSection
local CharacterSection = BlueTab:AddSection({
	Name = "Character"
})
BlueTab:AddTextbox({
	Name = "WalkSpeed (Default 16)",
	Default = "16",
	TextDisappear = true,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end	  
})
BlueTab:AddTextbox({
	Name = "Jump Power (Default 50)",
	Default = "50",
	TextDisappear = true,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end	  
})
BlueTab:AddButton({
	Name = "Infinite Jump",
	Callback = function()
          loadstring(game:HttpGet("https://pastebin.com/raw/u9EuU3FE"))()
	end   
})
